
            {data && <table>
              <thead>
                <tr>
                  {data[0] && data[0].map((h, i) => <th key={i}>{h}</th>)}
                </tr>
              </thead>
              <tbody>
                {data.slice(1).map((row, i) => <tr key={i}>
                  {row.map((cell, j) => <td key={j}>{cell}</td>)}
                </tr>)}
              </tbody>
            </table>}